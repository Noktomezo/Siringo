        "distube": "^4.0.4",
        "ffmpeg-static": "^5.1.0",
        "keyv": "^4.5.2",
        "libsodium-wrappers": "^0.7.11",
        "tslib": "^2.5.0",
        "utf-8-validate": "^6.0.3",
        "zlib-sync": "^0.1.8"
    },
    "devDependencies": {
